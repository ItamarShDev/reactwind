{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "declaration": true,
    "declarationMap": true,
    "outDir": "dist",
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "types": [],
    "baseUrl": ".",
    "paths": {
      "reactwind": [
        "./src/index.ts"
      ],
      "reactwind/jsx-runtime": [
        "./src/jsx-runtime.ts"
      ],
      "reactwind/jsx-dev-runtime": [
        "./src/jsx-dev-runtime.ts"
      ]
    }
  },
  "include": [
    "src"
  ]
}