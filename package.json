{
  "name": "@itamarshdev/reactwind",
  "version": "0.1.1",
  "description": "JSX runtime wrapper that adds a classNames prop to React elements.",
  "keywords": [
    "react",
    "tailwind",
    "tailwindcss",
    "jsx",
    "runtime",
    "styling",
    "css-in-js",
    "shorthand"
  ],
  "homepage": "https://itamarshdev.github.io/reactwind/storybook/",
  "bugs": {
    "url": "https://github.com/itamarshdev/reactwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itamarshdev/reactwind.git"
  },
  "license": "MIT",
  "author": "itamarshdev",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js",
      "require": "./dist/jsx-dev-runtime.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/jsx-runtime.ts src/jsx-dev-runtime.ts --format esm,cjs --dts",
    "prepublishOnly": "bun run build && bun run test",
    "test": "vitest run",
    "test:storybook": "vitest run --project storybook",
    "lint": "oxlint",
    "format": "oxfmt --write src",
    "check": "oxfmt --check src",
    "test:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/addon-docs": "10.2.1",
    "@storybook/addon-vitest": "^10.2.1",
    "@storybook/react-vite": "10.2.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "autoprefixer": "^10.4.23",
    "happy-dom": "^20.4.0",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "storybook": "10.2.1",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.5.1",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "packageManager": "bun@1.3.2",
  "module": "./dist/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
